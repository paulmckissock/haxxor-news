<tr class="header-space"></tr>
<tr>
  <td>
    <h1>Edit Email</h1>

    <%= form_with(model: user, url: update_email_user_path(user), method: :patch) do |form| %>
      <div>
        <%= form.label :email %><br>
        <%= form.email_field :email %>
      </div>

      <div>
        <%= form.submit "Update Email" %>
      </div>
    <% end %>
  </td>
</tr>

